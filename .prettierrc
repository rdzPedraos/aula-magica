{
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ],
    "printWidth": 80,
    "tabWidth": 4,
    "endOfLine": "lf",
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": true
}
